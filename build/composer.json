{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of hit.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "Extend Daterange functionality": "https://www.drupal.org/files/issues/2794481-50.patch"
      },
      "drupal/samlauth": {
        "Ignore Role Restrictions": "/build/patches/samlauth_rolematch.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/hit.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/itsjavi/fontawesome-iconpicker/archive/refs/tags/1.4.1.zip"
        },
        "name": "npm-asset/fontawesome-iconpicker",
        "type": "drupal-library",
        "version": "1.4.1"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/5.14.0.zip"
        },
        "extra": {
          "installer-name": "fontawesome"
        },
        "name": "unb-libraries/font_awesome",
        "type": "drupal-library",
        "version": "5.14.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/jackmoore/colorbox/archive/1.x.zip"
        },
        "extra": {
          "installer-name": "colorbox"
        },
        "name": "unb-libraries/colorbox",
        "type": "drupal-library",
        "version": "1.x-dev"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "3.1.0",
    "drupal/bootstrap4": "2.1.14",
    "drupal/captcha": "1.3",
    "drupal/colorbox": "1.10",
    "drupal/core": "9.4.2",
    "drupal/editor_file": "1.6",
    "drupal/entity_browser": "2.7",
    "drupal/facets": "2.0.4",
    "drupal/file_management": "1.x-dev",
    "drupal/fontawesome": "2.23",
    "drupal/fontawesome_iconpicker": "^2.0@RC",
    "drupal/google_analytics": "2.5",
    "drupal/health_check": "1.3",
    "drupal/honeypot": "2.1.1",
    "drupal/media_entity_browser": "2.0-alpha3",
    "drupal/pathauto": "1.10",
    "drupal/recaptcha": "3.0",
    "drupal/redis": "1.5",
    "drupal/samlauth": "3.6",
    "drupal/search_api": "1.23",
    "drupal/search_api_autocomplete": "1.6",
    "drupal/search_api_solr": "4.2.7",
    "drupal/token": "1.10",
    "drupal/twig_tweak": "2.9",
    "drupal/video_embed_field": "2.4",
    "npm-asset/fontawesome-iconpicker": "^1.3",
    "unb-libraries/colorbox": "1.x-dev",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/font_awesome": "~5.14",
    "unb-libraries/saml_features": "dev-8.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
